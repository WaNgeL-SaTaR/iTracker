<p id="notice"><%= notice %></p>

<h1>Sprint Teams</h1>

<table>
  <thead>
    <tr>
      <th>Sprint</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sprint_teams.each do |sprint_team| %>
      <tr>
        <td><%= sprint_team.sprint %></td>
        <td><%= sprint_team.team %></td>
        <td><%= link_to 'Show', sprint_team %></td>
        <td><%= link_to 'Edit', edit_sprint_team_path(sprint_team) %></td>
        <td><%= link_to 'Destroy', sprint_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sprint Team', new_sprint_team_path %>
